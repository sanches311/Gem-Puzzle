{
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "es2021": true ,
        "node": true

    },
    "extends":"airbnb",
    "rules": {
        "no-debugger": "off",
       // "no-console": 0,
        "class-methods-use-this": "off",
        //"@typescript-eslint/rule-name": "error"
        //"linebreak-style": ["error", "windows"],
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]

    }

}